<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >
    <com.example.administrator.el_done1.TreasureDecorationView
        android:id="@+id/treasure_decoration_0_0"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/d6_button"
        android:layout_margin="10dp"
        />
    <com.example.administrator.el_done1.TreasureDecorationView
        android:id="@+id/treasure_decoration_0_1"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/d5_button"
        android:layout_margin="10dp"
        android:layout_toRightOf="@id/treasure_decoration_0_0"/>
    <com.example.administrator.el_done1.TreasureDecorationView
        android:id="@+id/treasure_decoration_1_0"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/d4_button"
        android:layout_margin="10dp"
        android:layout_below="@id/treasure_decoration_0_0"
        />
    <com.example.administrator.el_done1.TreasureDecorationView
        android:id="@+id/treasure_decoration_1_1"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/d3_button"
        android:layout_margin="10dp"
        android:layout_toRightOf="@id/treasure_decoration_1_0"
        android:layout_below="@id/treasure_decoration_0_1"
        />
    <com.example.administrator.el_done1.TreasureDecorationView
        android:id="@+id/treasure_decoration_2_0"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/d2_button"
        android:layout_margin="10dp"
        android:layout_below="@id/treasure_decoration_1_0"
        />
    <com.example.administrator.el_done1.TreasureDecorationView
        android:id="@+id/treasure_decoration_2_1"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/d1_button"
        android:layout_margin="10dp"
        android:layout_toRightOf="@id/treasure_decoration_2_0"
        android:layout_below="@id/treasure_decoration_1_1"
        />
</RelativeLayout>